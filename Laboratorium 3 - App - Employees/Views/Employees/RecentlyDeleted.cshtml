@model List<RecentlyDeletedEmployee>

<h2>Ostatnio Usunięci Pracownicy</h2>

<table>
    <thead>
        <tr>
            <th>Imię</th>
            <th>Nazwisko</th>
            @* <th>Pesel</th>
            <th>Stanowisko</th>
            <th>Department</th>
            <th>Hire</th>
            <th>Fire</th> *@
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Name</td>
                <td>@employee.Surname</td>
                @* <td>@employee.Pesel</td>
                <td>@employee.Stanowisko</td>
                <td>@employee.Department</td>
                <td>@employee.Hire</td>
                <td>@employee.Fire</td> *@
            </tr>
        }
    </tbody>
</table>
